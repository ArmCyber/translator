{
  "name": "zakhayko/translator",
  "description": "Translator for Laravel",
  "license": "MIT",
  "homepage": "https://github.com/zakhayko/translator",
  "authors": [
    {
      "name": "Hayko",
      "email": "zakhayko@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=7.1",
    "laravel/framework": ">=5.5"
  },
  "autoload": {
    "psr-4": {
      "Zakhayko\\Translator\\": "src"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "config": {
    "preferred-install": "dist"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Zakhayko\\Translator\\ServiceProvider"
      ]
    }
  }
}
